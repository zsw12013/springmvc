<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
  
  <classPathEntry location="D:\maven\repository\mysql\mysql-connector-java\5.1.34\mysql-connector-java-5.1.34.jar" />
  
  <context id="base" targetRuntime="MyBatis3" defaultModelType="conditional">
	<commentGenerator>
		<property name="suppressAllComments" value="false" />
	</commentGenerator>
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://127.0.0.1:3306/csdn?useUnicode=true&amp;characterEncoding=UTF-8" userId="root" password="root" />
     
    
    <javaTypeResolver>  
	     <property name="forceBigDecimals" value="false" />  
    </javaTypeResolver>
    <javaModelGenerator targetPackage="cn.bishiti.base.model" targetProject="base" >
         <property name="enableSubPackages" value="true"/>  
         <property name="trimStrings" value="true"/>
    </javaModelGenerator>
    <sqlMapGenerator targetPackage="mybatis.mappers" targetProject="base/src/main/resources">
    	<property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
    <javaClientGenerator targetPackage="cn.bishiti.base.dao" targetProject="base" type="XMLMAPPER">
    	<property name="enableSubPackages" value="true" />    	
    </javaClientGenerator>
    
     <table schema="base" tableName="users" 
    		enableInsert="true" enableSelectByPrimaryKey="true"
			enableUpdateByPrimaryKey="true" enableDeleteByPrimaryKey="true"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="true"  >
	</table>
  </context>
</generatorConfiguration>